{
  "name": "@nordicsemiconductor/e2e-bdd-test-runner-example",
  "version": "0.0.0-development",
  "description": "Example use of @nordicsemiconductor/bdd-markdown",
  "type": "module",
  "scripts": {
    "test": "$(set -o pipefail && npx tsx features/run-features.ts | npx tsx features/console-reporter.ts)",
    "test:unit": "npx tsx --test ./features/*.spec.ts",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NordicSemiconductor/cloud-bdd-markdown-e2e-example-aws-js.git"
  },
  "keywords": [
    "bdd",
    "aws",
    "cloudnative",
    "e2e",
    "bdd",
    "testing"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/NordicSemiconductor/cloud-bdd-markdown-e2e-example-aws-js/issues"
  },
  "homepage": "https://github.com/NordicSemiconductor/cloud-bdd-markdown-e2e-example-aws-js#readme",
  "devDependencies": {
    "@aws-sdk/client-cloudformation": "3.511.0",
    "@aws-sdk/client-sqs": "3.511.0",
    "@aws-sdk/client-xray": "3.511.0",
    "@commitlint/config-conventional": "18.6.1",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "13.1.15",
    "@nordicsemiconductor/bdd-markdown": "8.0.5",
    "@nordicsemiconductor/cloudformation-helpers": "9.0.2",
    "@nordicsemiconductor/eslint-config-asset-tracker-cloud-typescript": "17.0.0",
    "@sinclair/typebox": "0.32.14",
    "@swc/core": "1.4.1",
    "@types/aws-lambda": "8.10.133",
    "@types/glob": "8.1.0",
    "@types/node": "20.11.17",
    "@types/yazl": "2.4.5",
    "@typescript-eslint/eslint-plugin": "7.0.1",
    "aws-cdk-lib": "2.127.0",
    "cdk": "2.127.0",
    "constructs": "10.3.0",
    "dependency-tree": "10.0.9",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-unicorn": "51.0.1",
    "glob": "10.3.10",
    "husky": "9.0.10",
    "node-fetch": "3.3.2",
    "tsx": "4.7.1",
    "yazl": "2.5.1"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ],
    "*.{md,json,yaml,yml,js}": [
      "prettier --write"
    ]
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/github",
        {
          "successComment": false,
          "failTitle": false
        }
      ]
    ]
  },
  "engines": {
    "node": ">=20",
    "npm": ">=9"
  },
  "prettier": "@nordicsemiconductor/asset-tracker-cloud-code-style/.prettierrc",
  "dependencies": {
    "@aws-lambda-powertools/tracer": "1.18.0"
  }
}
