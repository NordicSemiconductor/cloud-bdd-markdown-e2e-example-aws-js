{
  "name": "@nordicsemiconductor/e2e-bdd-test-runner-example",
  "version": "0.0.0-development",
  "description": "Example use of @nordicsemiconductor/bdd-markdown",
  "type": "module",
  "scripts": {
    "test": "$(set -o pipefail && npx tsx features/run-features.ts | npx tsx features/console-reporter.ts)",
    "test:unit": "jest",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NordicSemiconductor/cloud-bdd-markdown-e2e-example-aws-js.git"
  },
  "keywords": [
    "bdd",
    "aws",
    "cloudnative",
    "e2e",
    "bdd",
    "testing"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/NordicSemiconductor/cloud-bdd-markdown-e2e-example-aws-js/issues"
  },
  "homepage": "https://github.com/NordicSemiconductor/cloud-bdd-markdown-e2e-example-aws-js#readme",
  "devDependencies": {
    "@aws-sdk/client-cloudformation": "3.413.0",
    "@aws-sdk/client-sqs": "3.413.0",
    "@aws-sdk/client-xray": "3.413.0",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "12.0.86",
    "@nordicsemiconductor/bdd-markdown": "6.0.0",
    "@nordicsemiconductor/cloudformation-helpers": "8.0.0",
    "@sinclair/typebox": "0.31.15",
    "@swc/core": "1.3.84",
    "@swc/jest": "0.2.29",
    "@types/aws-lambda": "8.10.121",
    "@types/glob": "8.1.0",
    "@types/jest": "29.5.4",
    "@types/node": "20.6.0",
    "@types/yazl": "2.4.2",
    "aws-cdk-lib": "2.96.1",
    "cdk": "2.96.1",
    "constructs": "10.2.70",
    "dependency-tree": "10.0.9",
    "eslint-plugin-import": "2.28.1",
    "glob": "10.3.4",
    "jest": "29.7.0",
    "node-fetch": "3.3.2",
    "tsx": "3.12.10",
    "yazl": "2.5.1"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ],
    "*.{md,json,yaml,yml,js}": [
      "prettier --write"
    ]
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/github",
        {
          "successComment": false,
          "failTitle": false
        }
      ]
    ]
  },
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "prettier": "@nordicsemiconductor/asset-tracker-cloud-code-style/.prettierrc",
  "dependencies": {
    "@aws-lambda-powertools/tracer": "1.12.1"
  },
  "jest": {
    "transform": {
      ".+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    },
    "testRegex": ".+\\.spec\\.ts$"
  }
}
